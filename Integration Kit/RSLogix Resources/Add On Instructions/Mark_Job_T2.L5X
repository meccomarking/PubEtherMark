<Routines>
<Routine Name="Logic" Type="RLL">
<RLLContent>
<Rung Number="0" Type="N">
<Text>
<![CDATA[[XIC(S:FS) ,[XIC(Reset) ,EQU(999,Outputs.Marker_State) ] ]MOV(0,nstate)OTU(InProcess)OTU(Done);]]>
</Text>
</Rung>
<Rung Number="1" Type="N">
<Text>
<![CDATA[EQU(nstate,0)[EQU(0,Outputs.Marker_State) OTL(InProcess) MOV(200,Inputs.Marker_Command) MOV(1,nstate) ,NEQ(0,Outputs.Marker_State) NEQ(209,Outputs.Marker_State) NEQ(999,Outputs.Marker_State) TON(ErrorTimer,?,?) XIC(ErrorTimer.DN) OTL(Error) ,EQU(209,Outputs.Marker_State) MOV(1,nstate) ,EQU(999,Outputs.Marker_State) OTL(Error) MOV(0,nstate) ];]]>
</Text>
</Rung>
<Rung Number="2" Type="N">
<Text>
<![CDATA[EQU(nstate,1)[EQU(Outputs.Marker_State,209) MOV(0,Inputs.Marker_Command) EQU(0,Inputs.Marker_Command) OTL(Done) OTU(InProcess) ,EQU(Outputs.Marker_State,999) MOV(0,Inputs.Marker_Command) OTL(Done) OTL(Error) ,EQU(Outputs.Marker_State,0) ]MOV(0,nstate);]]>
</Text>
</Rung>
<Rung Number="3" Type="N">
<Text>
<![CDATA[EQU(Outputs.Marker_State,201)OTE(MarkInProgress);]]>
</Text>
</Rung>
<Rung Number="4" Type="N">
<Text>
<![CDATA[AFI()NEQ(nstate,1)EQU(1,Outputs.Marker_State)MOV(0,nstate);]]>
</Text>
</Rung>
